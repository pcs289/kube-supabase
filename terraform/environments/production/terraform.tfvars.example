##################
### Global
##################
environment = "production"

##########
### EKS
##########
cluster_name    = "production-cluster"
cluster_cidr    = "10.100.0.0/16"
cluster_version = "1.33"

##################
### Networking
##################
vpc_cidr_block = "10.0.0.0/16"
vpc_single_nat = true

allow_ssh_cidrs = [
  "MY_IP/32",
]

allow_ssh_keys = [
  "ssh-rsa SSH PUBLIC KEY"
]

################
#### RDS
################
db_identifier     = "production-database"
db_instance_class = "db.t3.small"
db_master_user    = "postgres"

db_engine         = "postgres"
db_engine_version = "17.2"

db_storage     = 20
db_max_storage = 50

db_params = [
  { name = "client_encoding", value = "utf8" },
  { name = "rds.force_ssl", value = "0" }
]

################
#### S3
################
bucket_name      = "supabase-s3"
bucket_encrypted = true
bucket_versioned = false

